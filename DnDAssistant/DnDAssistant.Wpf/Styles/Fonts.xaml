<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DnDAssistant.Wpf"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Fonts -->
    <FontFamily x:Key="FontAwasome">pack://application;,,,/Fonts/#FontAwesome</FontFamily>

    <!-- Fontsizes -->
    <system:Double x:Key="FontsizeRegular">18</system:Double>
    <system:Double x:Key="FontsizeExtraLarge">42</system:Double>

    <!-- FontAwesome Strings -->
    <system:String x:Key="FontAwesomeCogIcon">&#xf013;</system:String>

</ResourceDictionary>