<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DnDAssistant.Wpf">

    <Color x:Key="Black">#000000</Color>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}"/>

    <Color x:Key="Gray">#a9a9a9</Color>
    <SolidColorBrush x:Key="GrayBrush" Color="{StaticResource Gray}"/>

    <Color x:Key="White">#ffffff</Color>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>

    <Color x:Key="SmokeWhite">#f5f5f5</Color>
    <SolidColorBrush x:Key="SmokeWhiteBrush" Color="{StaticResource SmokeWhite}"/>

    <Color x:Key="Bronze">#98641A</Color>
    <SolidColorBrush x:Key="BronzeBrush" Color="{StaticResource Bronze}"/>

    <!--The reds-->
    <Color x:Key="LightRed">#c52e21</Color>
    <SolidColorBrush x:Key="LightRedBrush" Color="{StaticResource LightRed}"/>
    <Color x:Key="Red">#99241a</Color>
    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource Red}"/>
    <Color x:Key="DarkRed">#6d1a13</Color>
    <SolidColorBrush x:Key="DarkRedBrush" Color="{StaticResource DarkRed}"/>

    <Color x:Key="Blue">#328dcd</Color>
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource Blue}"/>

</ResourceDictionary>