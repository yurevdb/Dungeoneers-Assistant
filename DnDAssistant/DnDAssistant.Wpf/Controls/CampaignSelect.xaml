<Window x:Class="DnDAssistant.Wpf.CampaignSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DnDAssistant.Wpf"
        mc:Ignorable="d"
        WindowStyle="None"
        AllowsTransparency="True"
        Topmost="True"
        WindowStartupLocation="CenterScreen"
        Closing="Window_Closing"
        Title="CampaignSelect" Height="675" Width="1200">
    
    <Grid Background="{StaticResource SmokeWhiteBrush}" >
        
        <Button x:Name="btn" Visibility="Collapsed" Width="100" Height="100" Content="Klik" Click="Button_Click"/>
        
    </Grid>
    
</Window>
